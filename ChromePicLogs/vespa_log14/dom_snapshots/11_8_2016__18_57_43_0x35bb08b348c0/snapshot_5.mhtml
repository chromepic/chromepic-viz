.tocNav { z-index: 5000000; position: fixed; left: 0px; top: 0px; height: 100%; box-shadow: rgba(0, 0, 0, 0.0980392) -10px 0px 20px inset; max-width: 0px; width: 0px; overflow: hidden; }

.omnibar:not(.crawlAd) .tocNav { padding-top: 45px; box-sizing: border-box; }

.tocNav ul, .tocNav li, .tocNav .ad-native-toc { transition: margin-left 0.5s ease 0.25s; }

.tocNav ul { padding-top: 10px; height: calc((100% - 59px) - 10px); overflow-y: auto; overflow-x: hidden; position: relative; width: 100%; padding-right: 15px; box-shadow: rgba(0, 0, 0, 0.0980392) -25px 0px 20px inset; background-color: rgb(252, 252, 252); }

.tocNav li { margin-left: -7px; position: relative; }

.tocNav li:hover { box-shadow: rgba(0, 0, 0, 0.0980392) -10px 0px 20px inset; background-color: rgb(238, 238, 238); }

.tocNav .ad-native-toc { height: 158px; }

.tocNav a { padding: 10px 20px; height: 70px; outline: none; overflow: hidden; display: block; width: 260px; }

.tocNav figure.img { height: 70px; position: relative; width: 70px; }

.tocNav img { height: 70px; width: 70px; }

.tocNav .topStoriesHeading:hover { box-shadow: none; background-color: transparent; }

.tocNav .topStoriesHeading h5 { transition: all 0.5s ease 0.25s; color: rgb(184, 0, 0); font-size: 11px; padding: 0px 20px; text-transform: uppercase; width: 260px; }

.tocNav .desc { margin-left: 90px; width: 170px; overflow: hidden; font-size: 0.82353em; line-height: 1.14286em; max-height: 63px; position: relative; top: 50%; transform: translateY(-50%); }

.tocNav .hed { font-weight: 600; }

.tocNav .hed, .tocNav a:hover .hed, .tocNav .dek { color: rgb(0, 0, 0); }

.tocNav .selected a { cursor: default; }

.tocNav .selected figure::after { box-sizing: border-box; content: ""; border: 5px solid rgb(184, 0, 0); position: absolute; right: 0px; top: 0px; height: 70px; width: 70px; }

.tocNav .rbLogo { height: 59px; width: 94px; display: block; font-size: 0px; padding: 0px; transition: all 0.5s ease 0.25s; background-position: 20px 50%; }

.tocNav.auto { padding-bottom: 5px; }

.tocNav.auto ul { height: 100%; overflow-x: hidden; background-color: rgb(30, 30, 30); }

.omnibar:not(.crawlAd) .tocNav.auto { padding-top: 0px; }

.tocNav.auto .hed, .tocNav.auto a:hover .hed { color: rgb(255, 255, 255); }

.tocNav.auto .dek, .tocNav.auto a:hover .dek { color: rgb(108, 112, 115); }

.tocNav.auto .hed { font-weight: 400; }

.tocNav.auto .rbLogo { display: none; }

.tocNav.auto li:hover { background-color: rgb(42, 42, 42); }

.tocNav.auto li:last-of-type { padding-bottom: 10px; }

.tocNav.auto .ad-native-toc { height: 100px; }

.tocNavVisible { overflow-x: hidden; }

.tocNavVisible .tocNav { width: 95px; max-width: 300px; }

.tocNavVisible .tocNav, .tocNavVisible #rbContent, .tocNavVisible #rbHeader .fixed form, .tocNavVisible #primaryNav { transition: all 0.5s ease 0.25s; }

.tocNavVisible .headerContainer, .tocNavVisible .pageHeader { transition: all 0.5s ease 0.25s; margin-left: 95px; }

.tocNavVisible .headerContainer .primaryNav { transition: all 0.5s ease 0.25s; margin-right: 95px; }

.tocNavOpen .tocNav { width: 305px; }

.tocNavOpen .tocNav ul, .tocNavOpen .tocNav .ad-native-toc { margin-left: 0px; box-shadow: rgba(0, 0, 0, 0.0980392) -10px 0px 20px inset; }

.tocNavOpen .tocNav ul, .tocNavOpen .tocNav .rbLogo { width: 300px; }

.tocNavOpen .tocNav ul { padding-right: 0px; }

.tocNavOpen { overflow: hidden; }

.tocNavOpen #rbHeader .fixed form, .tocNavOpen #primaryNav { transform: translate3d(205px, 0px, 0px); }

.tocNavOpen .headerContainer, .tocNavOpen .pageHeader { margin-left: 300px; width: calc((100% - 300px) + 95px); }

.tocNavOpen .headerContainer #primaryNav { margin-right: 300px; }

.tocNavOpen #specialHeader .specialHeaderLogo { }

@media (max-width: 1770px) { 
  .tocNavOpen #rbContent { transform: translate3d(205px, 0px, 0px); }
}

@media (max-width: 1230px) and (min-width: 1160px) { 
  body .container:not(#footerMap) { padding-left: 30px; }
}

@media (max-width: 1360px) and (min-width: 1230px), (max-width: 1160px) { 
  .tocNav, .tocNavVisible .tocNav { width: 16px; }
  .tocNav .expand { top: 59px; right: 0px; position: absolute; height: 100%; width: 16px; z-index: 1; transition: opacity 0.35s ease; background-color: rgb(144, 17, 2); }
  .tocNav .expand div { position: absolute; right: 0px; top: 50%; margin-top: -37.5px; border-top-width: 8px; border-top-style: solid; border-top-color: rgb(144, 17, 2); border-right-width: 8px; border-right-style: solid; border-right-color: rgb(255, 255, 255); border-bottom-width: 8px; border-bottom-style: solid; border-bottom-color: rgb(144, 17, 2); }
  .tocNav.auto .expand { top: 0px; }
  .rbLogo { display: none; }
  .tocNavOpen .tocNav ul, .tocNavOpen .tocNav .rbLogo { display: block; }
  .tocNavOpen .tocNav ul { margin-left: 0px; }
  .tocNavOpen .tocNav .expand { opacity: 0; z-index: 0; }
  .tocNavOpen .tocNav.auto .rbLogo { display: none; }
  .tocNavOpen .headerContainer, .tocNavOpen .pageHeader { margin-left: 300px; }
  .tocNav .selected::after { content: none; }
  .tocNavVisible .tocNav ul { margin-left: -300px; }
  .tocNavOpen #rbContent { transform: translate3d(300px, 0px, 0px); }
  .tocNavOpen .tocNav { width: 300px; }
  .tocNavOpen .tocNav ul { margin-left: 0px; }
  .tocNavOpen .tocNav .selected::after { content: ""; }
  body:not(.auto) .tocNavVisible .headerContainer, body:not(.auto) .tocNavVisible .pageHeader { margin-left: 16px; }
}

