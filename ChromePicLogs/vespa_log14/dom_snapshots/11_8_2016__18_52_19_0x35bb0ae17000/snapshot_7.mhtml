.soutu-layer { position: absolute; border: 1px solid rgb(51, 133, 255); z-index: 101; background-color: rgb(255, 255, 255); }

.soutu-layer .soutu-icon { display: inline-block; background: url("http://s1.bdstatic.com/r/www/cache/static/soutu/img/soutu_icons_sample_6a1e1619.png") no-repeat; }

.soutu-layer .soutu-url-wrap { border-bottom-width: 1px; border-bottom-style: solid; border-bottom-color: rgb(204, 204, 204); display: inline-block; width: 539px; overflow: hidden; height: 34px; vertical-align: top; background: rgb(255, 255, 255); }

.soutu-env-nomac.soutu-env-index .soutu-layer .soutu-url-wrap { background: transparent; }

.soutu-layer .soutu-url-wrap.focus { border-color: rgb(71, 145, 255) transparent rgb(71, 145, 255) rgb(71, 145, 255); }

.soutu-layer #soutu-url-kw { width: 526px; height: 22px; font-style: normal; font-variant: normal; font-weight: normal; font-stretch: normal; font-size: 13px; line-height: 18px; font-family: 宋体; color: rgb(170, 170, 170); margin: 6px 0px 0px 7px; padding: 0px; border: 0px; outline: 0px; -webkit-appearance: none; background: transparent; }

.soutu-layer .soutu-url { position: relative; z-index: 2; }

.soutu-layer .soutu-url-btn { position: relative; display: inline-block; text-align: center; width: 100px; height: 35px; border-width: 0px 0px 1px; border-style: none none solid; border-bottom-color: rgb(45, 120, 244); cursor: pointer; vertical-align: top; margin: -1px -1px 0px 0px; background: rgb(51, 133, 255); }

.soutu-layer .soutu-url-btn:hover { border-bottom-width: 1px; border-bottom-style: solid; border-bottom-color: rgb(40, 104, 200); box-shadow: rgb(204, 204, 204) 1px 1px 1px; margin: -1px -1px 0px 0px; background: rgb(49, 126, 243); }

.soutu-layer .soutu-url-btn-icon { position: absolute; margin: auto; left: 0px; right: 0px; top: 0px; bottom: 0px; width: 18px; height: 16px; background-image: -webkit-image-set(url("http://s1.bdstatic.com/r/www/cache/static/soutu/img/camera_layer_1f770dcc.png") 1x, url("http://s1.bdstatic.com/r/www/cache/static/soutu/img/camera_layer_x2_57b494bd.png") 2x); }

.soutu-layer .soutu-state-normal { padding: 23px 20px 27px; }

.soutu-layer .soutu-close { position: absolute; right: 18px; bottom: 13px; z-index: 2; width: 18px; height: 18px; background-position: -20px 0px; }

.soutu-layer .soutu-close:hover { background-position: -197px -1px; }

.soutu-layer .soutu-url-error { position: absolute; right: 121px; margin-top: 5px; display: none; width: 134px; padding-left: 36px; height: 26px; line-height: 26px; color: rgb(238, 68, 51); background: url("http://s1.bdstatic.com/r/www/cache/static/soutu/img/soutu_icons_sample_6a1e1619.png") 100% -65px no-repeat; }

.soutu-layer .soutu-drop { text-align: center; height: 75px; padding-top: 25px; margin-bottom: 16px; background-color: rgb(250, 250, 250); }

.soutu-layer .soutu-drop.drag-over { background-color: rgb(242, 242, 242); }

.soutu-layer .soutu-drop-tip { display: block; font-size: 16px; color: rgb(153, 153, 153); }

.soutu-drop.drag-over .soutu-drop-tip { color: rgb(51, 51, 51); }

.soutu-layer .soutu-drop-icon { margin-top: 6px; width: 28px; height: 28px; background-position: -70px -2px; }

.soutu-drop.drag-over .soutu-drop-icon { background-position: -42px -2px; }

.soutu-layer .upload-wrap { position: relative; width: 158px; height: 43px; font-size: 16px; border: 1px solid rgb(202, 203, 204); line-height: 43px; margin: 0px auto; color: rgb(51, 51, 51); text-align: center; }

.soutu-layer .upload-wrap:hover { border-color: rgb(56, 139, 255); }

.soutu-layer .upload-pic { position: absolute; font-size: 0px; width: 100%; height: 100%; outline: 0px; opacity: 0; margin-left: -18px; z-index: 1; cursor: pointer; }

.soutu-layer .upload-icon { position: relative; top: 2px; width: 18px; height: 18px; margin-right: 8px; background-position: -144px 0px; }

.soutu-waiting { padding: 23px 20px 27px; margin-top: 35px; height: 125px; text-align: center; font-size: 16px; font-weight: bold; background: rgb(255, 255, 255); }

.soutu-waiting .ball { width: 40px; height: 10px; margin: 0px auto; position: relative; }

.soutu-waiting .b { left: 20px; position: absolute; width: 10px; height: 10px; border-radius: 50%; }

.soutu-waiting span { display: inline-block; margin-top: 30px; font-size: 16px; }

.soutu-error { padding: 23px 20px 27px; margin-top: 17px; height: 143px; text-align: center; }

.soutu-layer .soutu-error-icon { width: 44px; height: 44px; background-position: -98px -1px; }

.soutu-error-main { margin: 12px 0px 15px; font-style: normal; font-variant: normal; font-weight: normal; font-stretch: normal; font-size: 16px; line-height: normal; font-family: 'Microsoft YaHei'; color: rgb(51, 51, 51); }

.soutu-error-tip { color: rgb(153, 153, 153); font-style: normal; font-variant: normal; font-weight: normal; font-stretch: normal; font-size: 13px; line-height: normal; font-family: SimSun; }

.soutu-error-upload { text-decoration: underline; color: rgb(51, 51, 51); }

.soutu-error-upload:active { color: rgb(51, 51, 51); }

.soutu-error-upload:hover { cursor: pointer; }

