@charset "utf-8";

.mw-cite-backlink, .cite-accessibility-label { -webkit-user-select: none; }

sup.reference { unicode-bidi: isolate; white-space: nowrap; }

ol.references li:target, sup.reference:target { background-color: rgba(0, 127, 255, 0.133333); }

.mw-ext-cite-error { font-weight: bold; unicode-bidi: embed; }

@media print { 
  .mw-cite-backlink { display: none; }
}

.referencetooltip { position: absolute; list-style: none none; opacity: 0; font-size: 10px; margin: 0px; z-index: 5; padding: 0px; }

.referencetooltip li { border: 2px solid rgb(8, 0, 134); max-width: 260px; padding: 10px 8px 13px; margin: 0px; box-shadow: rgba(0, 0, 0, 0.298039) 2px 4px 2px; background-color: rgb(247, 247, 247); }

.referencetooltip li + li { margin-left: 7px; margin-top: -2px; border-width: 12px 7px 0px; padding: 0px; height: 3px; width: 0px; box-shadow: none; border-top-style: solid; border-top-color: rgb(8, 0, 134); border-right-style: solid; border-right-color: transparent; border-left-style: solid; border-left-color: transparent; background-color: transparent; }

.referencetooltip > li + li::after { content: ""; border-top-width: 8px; border-top-style: solid; border-top-color: rgb(247, 247, 247); border-right-width: 5px; border-right-style: solid; border-right-color: transparent; border-left-width: 5px; border-left-style: solid; border-left-color: transparent; margin-top: -12px; margin-left: -5px; z-index: 1; height: 0px; width: 0px; display: block; }

.client-js body .referencetooltip li li { border: none; box-shadow: none; height: auto; width: auto; margin: auto; padding: 0px; position: static; }

.RTflipped { padding-top: 13px; }

.referencetooltip.RTflipped li + li { position: absolute; top: 2px; border-top-width: 0px; border-bottom-width: 12px; border-bottom-style: solid; border-bottom-color: rgb(8, 0, 134); }

.referencetooltip.RTflipped li + li::after { border-top-width: 0px; border-bottom-width: 8px; border-bottom-style: solid; border-bottom-color: rgb(247, 247, 247); position: absolute; margin-top: 7px; }

.RTsettings { float: right; height: 24px; width: 24px; cursor: pointer; margin-top: -9px; margin-right: -7px; transition: opacity 0.15s; opacity: 0.6; background-image: linear-gradient(transparent, transparent), url("//upload.wikimedia.org/wikipedia/commons/7/77/Gear_icon.svg"); }

.RTsettings:hover { opacity: 1; }

.RTTarget { border: 2px solid rgb(8, 0, 134); }

